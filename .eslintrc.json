{
  "extends": [
    "@rocketseat/eslint-config/node",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "crlf",
        "singleQuote": true,
        "semi": false
      }
    ],
    "camelcase": "off",
    "no-useless-constructor": "off"
  }
}